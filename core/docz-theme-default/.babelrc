{
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "lodash",
    [
      "module-resolver",
      {
        "extensions": [".ts", ".tsx"],
        "root": ["./src"],
        "alias": {
          "~styles": "./src/styles",
          "~theme": "./src/theme",
          "~utils": "./src/utils"
        }
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": ["emotion"]
    },
    "development": {
      "plugins": [["emotion", { "sourceMap": true }]]
    }
  }
}
