{
  "plugins": [
    "lodash",
    [
      "module-resolver",
      {
        "extensions": [".ts", ".tsx"],
        "root": ["./src"],
        "alias": {
          "@utils": "./src/utils",
          "@styles": "./src/styles"
        }
      }
    ]
  ],
  "env": {
    "production": {
      "presets": ["@emotion/babel-preset-css-prop"]
    },
    "development": {
      "presets": [
        [
          "@emotion/babel-preset-css-prop",
          { "sourceMap": true, "autoLabel": true }
        ]
      ]
    }
  }
}
